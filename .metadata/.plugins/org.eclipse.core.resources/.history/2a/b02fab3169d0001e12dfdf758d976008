package com.Pattern;
import java.util.*;
public class Pattern {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
        Pattern1();
	}
	static void Pattern1{
		for(int i=0; i<n; i++) {
			for(int j=0; j<n; j++) {
				System.out.println("*");
			}
			System.out.println();
		}
	}

}
